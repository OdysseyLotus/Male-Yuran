<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RulePackDef>
		<defName>Yuranpawn_rule_Female</defName>
		<include>
			<li>YuranpawnName</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name(p=10)->[FemaleFirst] [Last]</li>
				<li>r_name(p=3)->[FemaleFirst] '[Nickname]' [Last]</li>
				<!--<li>r_name(p=2)->[FemaleFirst] '[Nickname]-[FemaleHonorific]' [Last]</li>-->
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	
