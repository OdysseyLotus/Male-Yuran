<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<XmlExtensions.SettingsMenuDef>
		<defName>MaleYuranSettings</defName> <!--Can be anything unique with no periods-->
		<modId>MaleYuran</modId> <!--MUST be the same as the one entered earlier-->    
		<label>Male Yuran</label> <!-- This is what displays as your mod's name-->    
		<settings> <!--This is where you define what settings are in the settings menu-->
			<li Class="XmlExtensions.Setting.Slider">
				<key>MaleYuranProbSlider</key>
				<label>Male Yuran: {MaleYuranProbSlider}. The chance for a male yuran to spawn, from 0 (0%) to 1 (100%). </label>
				<min>0.00</min>
				<max>1.00</max>
				<decimals>2</decimals>
			</li>
			<li Class="XmlExtensions.Setting.Slider">
				<key>MaleYuranMikoProbSlider</key>
				<label>Male Yuran Miko: {MaleYuranMikoProbSlider}. The chance for a male yuran miko to spawn, from 0 (0%) to 1 (100%). </label>
				<min>0.00</min>
				<max>1.00</max>
				<decimals>2</decimals>
			</li>
			<li Class="XmlExtensions.Setting.Slider">
				<key>MaleYuranSnakeProbSlider</key>
				<label>Male Yuran Black Snake: {MaleYuranSnakeProbSlider}. The chance for a male yuran black snake to spawn, from 0 (0%) to 1 (100%). </label>
				<min>0.00</min>
				<max>1.00</max>
				<decimals>2</decimals>
			</li>
			<li Class="XmlExtensions.Setting.ResetSettings"/>
		</settings>
	</XmlExtensions.SettingsMenuDef>

</Defs>